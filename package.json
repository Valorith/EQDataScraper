{
  "name": "eq-data-scraper-vue",
  "version": "1.0.0",
  "description": "EverQuest Spell Data Scraper - Vue 3 Edition with Railway Docker deployment - v2",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --host 0.0.0.0",
    "serve": "serve -s dist -p ${PORT:-3000} --host 0.0.0.0",
    "start": "npm run build && npm run serve",
    "stop": "python3 run.py stop",
    "status": "python3 run.py status",
    "install:all": "python3 run.py install"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "pinia": "^2.3.1",
    "vue": "^3.4.0",
    "vue-router": "^4.5.1",
    "serve": "^14.2.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.5.0",
    "vite": "^5.0.0"
  }
}
